	<div class="row">
		<div class="col-sm-2 top"><%= "Game ID"%> </div>
		<div class="col-sm-10 top"><%= form.number_field :id %></div>
	</div>

	<div class="row">
		<span class="col-sm-2 top"><%= form.label "Hand"%></span>
		<span class="col-sm-2 top"><%= form.text_area :yellowhand %></span>
		<span class="col-sm-2 top"><%= form.text_area :redhand %></span>
		<span class="col-sm-2 top"><%= form.text_area :greenhand %></span>
		<span class="col-sm-2 top"><%= form.text_area :bluehand %></span>
	    <span class="col-sm-2"></span>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Deck"%></div>
		<div class="col-sm-8 top"><%= form.text_area :deck, cols: "110", rows: "2"%></div>
	    <span class="col-sm-2"></span>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Board"%></div>
		<div class="col-sm-8 top"><%= form.text_area :board, cols: "110", rows: "2"%></div>
	    <span class="col-sm-2"></span>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Discard Pile"%></div>
		<div class="col-sm-8 top"><%= form.text_area :discardpile, cols: "110", rows: "2"%></div>
	    <span class="col-sm-2"></span>
	</div>


	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Comment"%></div>
		<div class="col-sm-8 top"><%= form.text_area :comment, cols: "110", rows: "2"%></div>
		<span class="col-sm-2"></span>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Message"%></div>
		<div class="col-sm-8 top"><%= form.text_area :message, cols: "110", rows: "2"%></div>
		<span class="col-sm-2"></span>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Screen Coordinates"%> </div>
		<div class="col-sm-10 top"><%= form.text_field :screen, :size=>"50"%></div>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Moved Marbles"%> </div>
		<div class="col-sm-10 top"><%= form.text_field :moved, :size=>"50"%></div>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Winner"%> </div>
		<div class="col-sm-10 top"><%= form.text_field :winner, :size=>"50"%></div>
	</div>

	<div class="row">
		<div class="col-sm-2 top"><%= form.label "Plays Made"%> </div>
		<div class="col-sm-10 top"><%= form.number_field :plays%></div>
	</div>

	<br>

